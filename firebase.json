{
    "hosting": {
        "rewrites": [{
                "source": "/index.html",
                "destination": "/PersonalWebsite/HTML/Home.html",
                "type": 301
            },
            {
                "source": "/",
                "destination": "/PersonalWebsite/HTML/Home.html",
                "type": 301
            }
        ],
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ]
    }
}